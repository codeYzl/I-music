<view class="all-menu">
  <block wx:for="{{menuList}}" wx:key="cat">
    <view class="cat">
      <area-header title="{{item.cat}}" hasMore = "{{false}}" />
    </view>
    <view class="list">
      <block wx:for="{{item.playlists}}" wx:key="id" wx:for-item="iten">
        <menu-item itemData="{{iten}}" class="iten"/>
      </block>
    </view>
  </block>
</view>