.status-bar{
    height: 40 px;
}

.nav{
    display: flex;
    height: 70px;
    color:#fff;
}

.left,.right,.center{
    display: flex;
    justify-content: center;
    align-items: center;
    
}
.nav .left, .nav .right{
    width: 120rpx;
}
.nav .center{
    flex: 1;
}

/*控制内容显示*/
.default{
    display: none;
}

.left .icon{
  
    width:48rpx;
    height: 48rpx;
}
.slot:empty + .default{
    display: flex;
}
