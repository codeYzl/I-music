<view class="song-item" bindtap="onSongClick">
  <image class="image" src="{{itemData.al.picUrl}}"></image>
  <view class="content">
    <view class="name">
      {{itemData.name}}
    </view>
    <view class="source">
    {{itemData.ar[0].name}}
    </view>
  </view>
  <view class="arrow">
    <van-icon name="arrow" color="#999" size="16rpx"></van-icon>
  </view>
</view>
